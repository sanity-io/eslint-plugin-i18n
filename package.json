{
  "name": "@sanity/eslint-plugin-i18n",
  "version": "1.1.0",
  "description": "This ESLint plugin provides rules to enforce specific code standards in internationalization practices, particularly focusing on attribute strings and template literals in JSX.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc && rollup -c",
    "prettify": "prettier -w .",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/eslint-plugin-i18n.git"
  },
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/eslint-plugin-i18n/issues"
  },
  "files": [
    "/dist",
    "./index-shim.d.ts"
  ],
  "keywords": [
    "sanity",
    "eslint",
    "eslintplugin"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@sanity/semantic-release-preset": "^4.1.6",
    "@types/eslint": "^8.44.8",
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^6.13.1",
    "@typescript-eslint/rule-tester": "^6.13.1",
    "@typescript-eslint/typescript-estree": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "rollup": "^4.6.1",
    "typescript": "^5.3.2"
  },
  "types": "./index-shim.d.ts",
  "exports": {
    ".": {
      "types": "./index-shim.d.ts",
      "default": "./dist/index.js"
    },
    "./defaults": {
      "types": "./dist/defaults.d.ts",
      "default": "./dist/defaults.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
